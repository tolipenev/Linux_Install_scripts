#: prefs.js/preferences/main.js:233
msgid "default"
msgstr "默认"

#: prefs.js/preferences/main.js:1004 prefs.js/ui/prefs.ui:1430
msgid "App Tweaks"
msgstr "应用优化"

#: prefs.js/preferences/main.js:1153
msgid "Add a Custom WM_CLASS"
msgstr "添加自定义 WM_CLASS"

#: prefs.js/ui/prefs.ui:11
msgid "Classy transparency for your panel."
msgstr "给你的面板加上优雅的透明感。"

#: prefs.js/ui/prefs.ui:13
msgid "Website"
msgstr "网站"

#: prefs.js/ui/prefs.ui:219
msgid "Shell Restart Required."
msgstr "需要重启 Shell。"

#. Meant to ask the user if they would like to restart right now or later in a yes/no format.
#: prefs.js/ui/prefs.ui:220
msgid ""
"A shell restart is required to view some of your changes. Do you want to "
"restart now?"
msgstr "为了观看你的一些更改的效果需要重启 shell。想现在就重启吗？"

#. How long the transition will take.
#: prefs.js/ui/prefs.ui:348
msgid "Transition Speed"
msgstr "切换速度"

#: prefs.js/ui/prefs.ui:378
msgid "Overrides 'gtk-enable-animations'."
msgstr "改写 'gtk-enable-animations'。"

#. Overrides Gnome animation settings and "forces" the animation.
#: prefs.js/ui/prefs.ui:391
msgid "Force animation"
msgstr "强制动画"

#. The function used to animate the panel traansition. See http://hosted.zeh.com.br/tweener/docs/en-us/misc/transitions.html
#: prefs.js/ui/prefs.ui:410
msgid "Transition Style"
msgstr "切换风格"

#: prefs.js/ui/prefs.ui:427
msgid "Linear (default)"
msgstr "线性（默认）"

#: prefs.js/ui/prefs.ui:428
msgid "Sine"
msgstr "正弦"

#: prefs.js/ui/prefs.ui:429
msgid "Quadratic"
msgstr "平方"

#: prefs.js/ui/prefs.ui:430
msgid "Cubic"
msgstr "立方"

#: prefs.js/ui/prefs.ui:431
msgid "Quartic"
msgstr "四阶"

#: prefs.js/ui/prefs.ui:432
msgid "Quintic"
msgstr "五阶"

#: prefs.js/ui/prefs.ui:433
msgid "Exponential"
msgstr "指数"

#: prefs.js/ui/prefs.ui:434
msgid "Circle"
msgstr "循环"

#: prefs.js/ui/prefs.ui:435
msgid "Elastic"
msgstr "弹性"

#: prefs.js/ui/prefs.ui:436
msgid "Bounce"
msgstr "跳动"

#: prefs.js/ui/prefs.ui:451
msgid "Transitions"
msgstr "切换"

#. Allows the user to set their own text color.
#: prefs.js/ui/prefs.ui:508
msgid "Enable custom text coloring"
msgstr "启用自定义文字颜色"

#. The main color to be used.
#: prefs.js/ui/prefs.ui:535 prefs.js/ui/prefs.ui:550
msgid "Primary Color"
msgstr "主色"

#. Another color for special functions.
#: prefs.js/ui/prefs.ui:564 prefs.js/ui/prefs.ui:578
msgid "Secondary Color"
msgstr "副色"

#: prefs.js/ui/prefs.ui:598
msgid "Use when a window is maximized"
msgstr "窗口最大化时使用"

#: prefs.js/ui/prefs.ui:621
msgid "Use when the overview is visible"
msgstr "概览视图时使用"

#: prefs.js/ui/prefs.ui:690 prefs.js/ui/prefs.ui:748 prefs.js/ui/prefs.ui:864
#: prefs.js/ui/prefs.ui:922
msgid "Shadow Color"
msgstr "阴影颜色"

#: prefs.js/ui/prefs.ui:761 prefs.js/ui/prefs.ui:935
msgid "Radius"
msgstr "半径"

#: prefs.js/ui/prefs.ui:774 prefs.js/ui/prefs.ui:948
msgid "Horizontal Offset"
msgstr "水平偏移"

#: prefs.js/ui/prefs.ui:787 prefs.js/ui/prefs.ui:961
msgid "Vertical Offset"
msgstr "垂直偏移"

#: prefs.js/ui/prefs.ui:800
msgid "Enable text shadowing"
msgstr "启用文字阴影"

#: prefs.js/ui/prefs.ui:974
msgid "Enable icon shadowing"
msgstr "启用图标阴影"

#: prefs.js/ui/prefs.ui:1023
msgid "Foreground"
msgstr "前景"

#. Opacity of the panel when no window is maximized.
#: prefs.js/ui/prefs.ui:1085
msgid "Unmaximized Opacity"
msgstr "非最大化的不透明度"

#. Opacity of the panel when a window is maximized.
#: prefs.js/ui/prefs.ui:1132 prefs.js/ui/app-prefs.ui:117
msgid "Maximized Opacity"
msgstr "最大化的不透明度"

#. Allows the user to set their own opacity.
#: prefs.js/ui/prefs.ui:1154
msgid "Enable custom opacity"
msgstr "启用自定义不透明度"

#. The panel's background color.
#: prefs.js/ui/prefs.ui:1221 prefs.js/ui/prefs.ui:1236
#: prefs.js/ui/app-prefs.ui:91
msgid "Panel Color"
msgstr "面板颜色"

#. Allows the user to set their own background color for the panel.
#: prefs.js/ui/prefs.ui:1257
msgid "Enable custom panel color"
msgstr "启用自定义面板颜色"

#: prefs.js/ui/prefs.ui:1311
msgid "Fixes theme incompatibilities."
msgstr "修复主题的不兼容性。"

#. Removes any extra styling certain themes may have. Fixes theme issues.
#: prefs.js/ui/prefs.ui:1318
msgid "Remove excess panel styling"
msgstr "移除过度的面板修饰风格"

#. Hides the panel corners.
#: prefs.js/ui/prefs.ui:1340
msgid "Hide corners"
msgstr "隐藏圆角"

#: prefs.js/ui/prefs.ui:1368
msgid "Background"
msgstr "背景"

#: prefs.js/ui/prefs.ui:1470
msgid "About"
msgstr "关于"

#. Adds a 'custom' app tweak. Only visible on the App Tweaks page.
#: prefs.js/ui/prefs.ui:1528
msgid "Advanced..."
msgstr "高级…"

#: prefs.js/ui/prefs.ui:1564
msgid ""
"A WM_CLASS is a code that identifies a window. Use this setting if the "
"window you want custom settings for is not part of an application. You can "
"find a window's WM_CLASS with the command 'xprop WM_CLASS'. Enter the second "
"result from 'xprop WM_CLASS' below without quotation marks."
msgstr ""
"WM_CLASS 是用于标识窗口的代码。使用此项设置，如果您想自定义设置的窗口不是应用"
"程序的一部分。您可以用命令 'xprop WM_CLASS' 来查找窗口的 WM_CLASS。输入 "
"'xprop WM_CLASS' 下面的第二个结果，不带引号。"

#: prefs.js/ui/prefs.ui:1613
msgid "WM_CLASS cannot be blank."
msgstr "WM_CLASS 无法为空。"

#: prefs.js/ui/app-prefs.ui:51
msgid "Enable background app tweaks"
msgstr "启用后台应用优化"

#: prefs.js/ui/app-prefs.ui:130
msgid "Theme Source"
msgstr "主题来源"

#: prefs.js/ui/app-prefs.ui:190
msgid "Always trigger the panel."
msgstr "总是触发面板。"
