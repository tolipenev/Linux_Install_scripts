#!/bin/bash

sudo add-apt-repository -y ppa:leolik/leolik
sudo add-apt-repository -y ppa:nilarimogard/webupd8
sudo apt update

sudo apt install -y notify-osd

pkill notify-osd
sudo apt install -y notifyosdconfig



sudo add-apt-repository -y ppa:gsn/grabbelkiste

sudo apt-get update

sudo apt-get install -y notifyosdconfig
